<html>
	<head>
		<link rel="stylesheet" href="{{ url_for('static', filename='revealjs/css/reveal.css') }}">
		<link rel="stylesheet" href="{{ url_for('static', filename='revealjs/css/theme/night.css')}}">
        <title>{% block title %}{% endblock %}</title>
	</head>
	<body>
		<div class="reveal">
			<div class="slides">
                {% block song_slides %}
                {% endblock %}
			</div>
		</div>
		<script src="{{ url_for('static', filename='revealjs/js/reveal.js')}}"></script>
		<script>
			Reveal.initialize({
                slideNumber: true,
            });
		</script>
</html>
